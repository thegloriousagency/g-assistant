# Database
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Auth
JWT_SECRET="replace-with-strong-secret"
JWT_EXPIRES_IN="1d"
PORT=3001
FRONTEND_URL="http://localhost:3000"

# GA4 service account credentials
# Path is relative to the backend directory when running `npm run start:dev`
GOOGLE_APPLICATION_CREDENTIALS="keys/ga4-service-account.json"
